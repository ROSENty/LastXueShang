<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/Medium_create_database"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Create database"/>

    <Button
        android:id="@+id/Medium_intent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="启动Major"/>

    <Button
        android:id="@+id/Medium_categoryCsv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="读categoryCsv"/>

    <Button
        android:id="@+id/Medium_majorCsv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="读majorCsv"/>

    <Button
        android:id="@+id/Medium_delData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="删除数据"/>
</LinearLayout>
